apiVersion: v1
kind: Secret
metadata:
  name: {{ include "helm-chart-mssql.fullname" . }}
stringData:
  ACCEPT_EULA: {{ required ".Values.mssql.acceptEula required" .Values.mssql.acceptEula }}
  MSSQL_PID: {{ required ".Values.mssql.pid required" .Values.mssql.pid }}  
  MSSQL_SA_PASSWORD: {{ required ".Values.mssql.saPassword required" .Values.mssql.saPassword }}  
  TZ: {{ .Values.mssql.timezone }}